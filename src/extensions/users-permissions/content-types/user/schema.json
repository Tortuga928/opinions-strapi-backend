{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "user_ratings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-rating.user-rating",
      "mappedBy": "users_permissions_user"
    },
    "userRole": {
      "type": "enumeration",
      "enum": ["sysadmin", "reguser"],
      "default": "reguser",
      "required": true
    },
    "displayName": {
      "type": "string",
      "maxLength": 100
    },
    "bio": {
      "type": "text",
      "maxLength": 500
    },
    "avatarId": {
      "type": "string",
      "default": "turtle",
      "maxLength": 50
    },
    "accountStatus": {
      "type": "enumeration",
      "enum": ["Active", "Suspended", "Disabled", "Locked"],
      "default": "Active",
      "required": true
    },
    "forcePasswordReset": {
      "type": "boolean",
      "default": false
    },
    "accountExpiration": {
      "type": "datetime"
    },
    "adminNotes": {
      "type": "text",
      "private": true
    },
    "lastLoginAt": {
      "type": "datetime"
    },
    "loginCount": {
      "type": "integer",
      "default": 0
    },
    "emailVerified": {
      "type": "boolean",
      "default": false
    },
    "emailVerificationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "pendingEmail": {
      "type": "email",
      "configurable": false,
      "private": true
    },
    "permissionProfiles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::permission-profile.permission-profile",
      "mappedBy": "users"
    }
  }
}
