{
  "kind": "collectionType",
  "collectionName": "user_ratings",
  "info": {
    "singularName": "user-rating",
    "pluralName": "user-ratings",
    "displayName": "UserRating"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "rating": {
      "type": "integer",
      "required": true,
      "default": 5,
      "min": 0,
      "max": 10
    },
    "comments": {
      "type": "text",
      "maxLength": 256
    },
    "opinion": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::opinion.opinion",
      "inversedBy": "user_ratings"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "user_ratings"
    }
  }
}
